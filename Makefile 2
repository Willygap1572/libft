SOURCE	= ft_calloc.c ft_strchr.c ft_strrchr.c ft_strlcat.c ft_strlcpy.c ft_strlen.c ft_strncmp.c ft_strnstr.c ft_strdup.c ft_substr.c ft_strjoin.c ft_strtrim.c ft_split.c ft_putchar_fd.c ft_putstr_fd.c ft_putendl_fd.c ft_putnbr_fd.c ft_tolower.c ft_toupper.c ft_atoi.c ft_itoa.c ft_striteri.c ft_strmapi.c ft_isalpha.c ft_isdigit.c ft_isalnum.c ft_isascii.c ft_isprint.c ft_memset.c ft_memcpy.c ft_memmove.c ft_bzero.c ft_memchr.c ft_memcmp.c
HEADER	= 
OUT	= libft.a
CC	 = gcc
CFLAGS	 = -c -Wall -Werror -Wextra
LFLAGS	 = 
AR = ar rcs
NAME = libft.a

all: $(NAME)

$(NAME): $(OBJS)
	$(AR) $@ $^
